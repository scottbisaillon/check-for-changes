{
  "name": "check-for-changes",
  "version": "1.0.5",
  "private": true,
  "description": "Github Action that checks if there have been changes since the last commit for specified files.",
  "main": "lib/index.js",
  "scripts": {
    "build": "ncc build src/index.ts -o dist --source-map --license licenses.txt --minify",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build src/index.ts -o dist --source-map --license licenses.txt --minify",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scottbisaillon/check-for-changes"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Scott Bisaillon",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@actions/github": "^5.0.1",
    "minimatch": "^5.0.1",
    "simple-git": "^3.7.0"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/minimatch": "^3.0.5",
    "@types/node": "^16.10.5",
    "@typescript-eslint/parser": "^5.22.0",
    "@vercel/ncc": "^0.31.1",
    "eslint": "^7.32.0",
    "eslint-plugin-github": "^4.3.2",
    "eslint-plugin-jest": "^26.1.5",
    "jest": "^27.2.5",
    "js-yaml": "^4.1.0",
    "prettier": "2.6.2",
    "ts-jest": "^27.1.2",
    "typescript": "^4.4.4"
  }
}
